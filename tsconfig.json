{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "downlevelIteration": true,
    "experimentalDecorators": true,
    "module": "esnext",
    "moduleResolution": "node",
    "importHelpers": true,
    "target": "es2015",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2018",
      "dom"
    ],
    "paths": {
      "oswis-ng-core": [
        "dist/oswis-ng-core/oswis-ng-core",
        "dist/oswis-ng-core"
      ],
      "oswis-ng-material": [
        "dist/oswis-ng-material/oswis-ng-material",
        "dist/oswis-ng-material"
      ],
      "oswis-ng-auth": [
        "dist/oswis-ng-auth/oswis-ng-auth",
        "dist/oswis-ng-auth"
      ],
      "oswis-ng-address-book": [
        "dist/oswis-ng-address-book/oswis-ng-address-book",
        "dist/oswis-ng-address-book"
      ]
    }
  },
  "angularCompilerOptions": {
    "fullTemplateTypeCheck": true,
    "strictInjectionParameters": true
  }
}